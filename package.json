{
  "name": "manaby-theme-talk-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:markup": "markuplint 'src/**/*.{html,js,ts,jsx,tsx}'",
    "lint": "npm run --silent lint:es; npm run --silent lint:markup",
    "format": "prettier --write --loglevel=warn './**/*.{js,jsx,ts,tsx,html,json}'",
    "fix": "npm run --silent lint:es:fix",
    "preinstall": "npx typesync || :",
    "prepare": "npx simple-git-hooks || :"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "markuplint",
      "eslint --fix ",
      "prettier --write --loglevel=error"
    ],
    "*.{css,scss,js,jsx,ts,tsx,json}": "prettier --write --loglevel=error"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-form": "^0.0.2",
    "@radix-ui/react-select": "^1.2.1",
    "firebase": "^9.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.6.1",
    "@markuplint/react-spec": "^3.7.0",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "lint-staged": "^13.2.1",
    "markuplint": "^3.8.0",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  }
}
